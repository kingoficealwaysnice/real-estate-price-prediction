{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "excludeFiles": "{models/**,data/**,notebooks/**,*.pkl,*.joblib,*.h5,*.hdf5,*.csv,*.xlsx,*.parquet,*.zip,*.tar.gz,*.log,*.tmp,*.cache,node_modules/**,.git/**,.pytest_cache/**,__pycache__/**,*.pyc,*.pyo,*.pyd}"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/vercel_app.py"
    }
  ]
}